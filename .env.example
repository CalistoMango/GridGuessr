# Application
NEXT_PUBLIC_URL=https://your-domain.vercel.app
NEXTAUTH_URL=https://your-domain.vercel.app
NEXT_PUBLIC_SHARE_URL=https://farcaster.xyz/miniapps/your-app-id/gridguessr
NEXT_PUBLIC_SHARE_HANDLE=@yourusername

# Supabase (get these from dashboard after setup)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Admin FIDs (your Farcaster ID)
ADMIN_FIDS=your-farcaster-id
ADMIN_PASSWORD=your-secure-admin-password

# Neynar API
NEYNAR_API_KEY=your-neynar-api-key
NEYNAR_CLIENT_ID=your-neynar-client-id
# Alternative names (FARCASTER_API_KEY, FARCASTER_CLIENT_ID also supported)

# Signer UUID @gridguessr account
NEYNAR_SIGNER_UUID=your-neynar-signer-uuid
# Alternative name (FARCASTER_SIGNER_UUID, FARCASTER_NEYNAR_SIGNER_UUID also supported)
FARCASTER_DEFAULT_CHANNEL_ID=
FARCASTER_DRY_RUN=false

# Vercel cron job secret
CRON_SECRET=your-secure-cron-secret

# Default values for local development
# For production, update these URLs to your deployed domain
KV_REST_API_TOKEN=
KV_REST_API_URL=
USE_TUNNEL=false

# Optional: Neynar signer sponsorship (prompted by deploy script)
# SEED_PHRASE=your-seed-phrase-for-signer-sponsorship
# SPONSOR_SIGNER=true
